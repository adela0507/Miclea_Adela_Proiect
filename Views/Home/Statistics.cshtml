@model IEnumerable<Miclea_Adela_Proiect.ProductViewModels.OrderGroup>
@{
    ViewData["Title"] = "Ordered Productss Statistics";
}
<h2>Ordered Products Statistics</h2>
<table>
    <tr>
        <th>
            Order Date
        </th>
        <th>
            Ordered Product
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @item.ProductCount
            </td>
        </tr>
    }
</table>